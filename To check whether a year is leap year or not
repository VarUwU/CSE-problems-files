#include<stdio.h>
int main()
{int a; 
printf("Enter the year that you want to find out whether it is leap year or not. \n");
scanf("%d",&a);
if(a%4==0)
{printf("The year %d is a leap year!",a);}
else
{
printf("The year %d is not a leap year! Next leap year is on year %d and previous leap year is on year %d .",a,(a-(a%4))+4,a-(a%4));
}
	return 0;
}
