#include<stdio.h>
int main()
{
	int a,b,c;
	printf("Enter the 3 numbers with a space:\n");
	scanf("%d %d %d",&a,&b,&c);
	if(a>b && a>c)
	{
		printf("%d is the greatest of the 3 numbers.",a);
	}
	else if(b>a && b>c)
	{
		printf("%d is the greatest of the 3 numbers.",b);
	}
	else
		printf("%d is the greatest of the 3 numbers.",c);
	return 	0;
}
